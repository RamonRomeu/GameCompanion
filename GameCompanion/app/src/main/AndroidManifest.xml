<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.gamecompanion">


<!--camera-->
    <uses-feature android:name="android.hardware.camera"
                  android:required="false" />

<!--permis guardar foto gran-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.RegisterActivity" android:label="Register" android:parentActivityName="com.example.gamecompanion.activity.MainActivity">
        </activity>
        <activity android:name="com.example.gamecompanion.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="ca-app-pub-7842353226072543~5633578208"/>

    </application>

</manifest>